package Days;
import java.util.*;
import java.io.*;

public class Day4 {
    public static void main(String[] args) {
        File file = new File("");
        
        try{
            Scanner src = new Scanner(file);
            while(src.hasNextLine()){
                System.out.println(src.nextLine());
            }
            
        }catch(FileNotFoundException ex){
            ex.printStackTrace();
            
        }
    }
}
