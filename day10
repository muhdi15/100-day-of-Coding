package mycode;

import java.util.Arrays;

public class MyCode {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {

        String in = "menyayangi";
        String[] ch;
        for (int i = 0; i < in.length(); i++) {
            System.out.print("'" + in.charAt(i) + "'" + ",");

        }
        System.out.println("");

    }

}
